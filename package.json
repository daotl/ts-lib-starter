{
  "name": "@daotl/ts-lib-starter",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@8.6.0",
  "description": "TypeScript Library Starter for DAOT projects",
  "author": "Nex Zhu <hitnexup@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/daotl/ts-lib-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daotl/ts-lib-starter.git"
  },
  "bugs": {
    "url": "https://github.com/daotl/ts-lib-starter/issues"
  },
  "keywords": [
    "TypeScript",
    "library"
  ],
  "packemon": {
    "format": [
      "esm",
      "cjs"
    ],
    "platform": [
      "node",
      "browser"
    ],
    "api": "public"
  },
  "main": "./cjs/index.cjs",
  "module": "./esm/index.js",
  "browser": "./lib/browser/index.js",
  "types": "./cjs/index.d.ts",
  "files": [
    "cjs/**/*",
    "dist",
    "esm/**/*",
    "lib/**/*",
    "src/**/*"
  ],
  "scripts": {
    "build": "packemon build --declaration",
    "pack": "packemon pack --addEngines --addFiles --declaration",
    "test": "jest",
    "lint": "rome format . && rome check .",
    "lint:fix": "rome format --write . && rome check --apply .",
    "gen-rome-json": "gen-rome-json",
    "pub": "npm run lint && npm run test && npm run build && npm publish --access public"
  },
  "devDependencies": {
    "@anatine/esbuild-decorators": "^0.2.19",
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@daotl/rome-config": "0.0.1-alpha.4",
    "@daotl/tsconfig": "^0.1.2",
    "@swc/core": "^1.3.42",
    "@types/jest": "29.5.0",
    "@types/node": "^18.15.11",
    "esbuild": "^0.17.14",
    "jest": "^29.5.0",
    "lefthook": "^1.3.8",
    "packemon": "^3.1.1",
    "pnpm": "^8.1.0",
    "rename-tool": "^1.1.1",
    "rome": "^12.0.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.2"
  }
}
